#include <iostream>

using namespace std;

int main()
{
string s;
cin >> s;
int num = s.length();
for(int i=0; i<num; i++)
 {
   if (s[i]>='a' && s[i] <= 'z')
    {
       s[i] -= 32;
       cout << s[i];
    }
   else if (s[i] >='A' && s[i] <= 'Z')
    {
       s[i] += 32;
       cout << s[i];
    }

   else if (s[i] == ',')
    {
       cout << " ";
    }
 }


    return 0;
}



// input >==   happy,NewYear,enjoy
// output >==  HAPPY nEWyEAR ENJOY
